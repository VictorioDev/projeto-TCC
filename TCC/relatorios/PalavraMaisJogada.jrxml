	<queryString language="SQL">
		<![CDATA[SELECT
	p.idPalavra,
	p.nome,
	count(*) as NumVezesJogadas,
	DATE_FORMAT(pj.`data`,'%d/%m/%Y') as DataJogo,
	sum(pj.acertou) QtdeAcertos,
	(sum(pj.palpites) / count(*)) as MediaPalpites

FROM
	palavrajogada pj,
	palavra p
WHERE
	pj.idPalavra = p.idPalavra
GROUP BY
	pj.idPalavra
ORDER BY
NumVezesJogadas desc]]>
	</queryString>
